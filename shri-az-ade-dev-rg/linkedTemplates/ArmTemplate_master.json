{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shri-az-ade-dev-rg"
		},
		"DiMiLaptopOnPremiesesSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DiMiLaptopOnPremiesesSQL'"
		},
		"ls_shri_adf_dev_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_shri_adf_dev_blob'"
		},
		"prj_azure_sqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'prj_azure_sqldb'"
		},
		"DiMiLaptopOnPremiesesSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "NB-SHRINIVAS"
		},
		"DiMiLaptopOnPremiesesSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureDataEngineering"
		},
		"DiMiLaptopOnPremiesesSQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"prj_azure_sqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shriadedevsql.database.windows.net"
		},
		"prj_azure_sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shriadcdevdb"
		},
		"prj_azure_sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa_adminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shri-az-ade-dev-rg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_shri_adf_dev_blob_connectionString": {
						"value": "[parameters('ls_shri_adf_dev_blob_connectionString')]"
					},
					"prj_azure_sqldb_password": {
						"value": "[parameters('prj_azure_sqldb_password')]"
					},
					"DiMiLaptopOnPremiesesSQL_password": {
						"value": "[parameters('DiMiLaptopOnPremiesesSQL_password')]"
					},
					"prj_azure_sqldb_properties_typeProperties_server": {
						"value": "[parameters('prj_azure_sqldb_properties_typeProperties_server')]"
					},
					"prj_azure_sqldb_properties_typeProperties_database": {
						"value": "[parameters('prj_azure_sqldb_properties_typeProperties_database')]"
					},
					"prj_azure_sqldb_properties_typeProperties_userName": {
						"value": "[parameters('prj_azure_sqldb_properties_typeProperties_userName')]"
					},
					"DiMiLaptopOnPremiesesSQL_properties_typeProperties_server": {
						"value": "[parameters('DiMiLaptopOnPremiesesSQL_properties_typeProperties_server')]"
					},
					"DiMiLaptopOnPremiesesSQL_properties_typeProperties_database": {
						"value": "[parameters('DiMiLaptopOnPremiesesSQL_properties_typeProperties_database')]"
					},
					"DiMiLaptopOnPremiesesSQL_properties_typeProperties_userName": {
						"value": "[parameters('DiMiLaptopOnPremiesesSQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}